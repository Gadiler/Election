package id_311217905_id_312126055;

import interfaces.Sickable;

public class SickContender extends Contender implements Sickable {

	public SickContender(String name, long id, String birthYear, Party<Citizen> p) throws Exception {
		super(name, id, birthYear, p);
	}

	@Override
	public String toString() {
		return super.toString() + (dayCount4Insulation == 0 ? ", Last day and i'm free!"
				: ", more " + dayCount4Insulation + " days to end the insulation");
	}
}
